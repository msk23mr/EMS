<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true"></constant>
	<package name="emp_manage_sys" extends="struts-default">

		<action name="index" class="org.apache.struts.action.LoginAction"
			method="input">
			<result name="input">/login.jsp</result>
			<result name="auth">/home.jsp</result>
		</action>

		<action name="login" class="org.apache.struts.action.LoginAction"
			method="execute">
			<result name="success">/home.jsp</result>
			<result name="input">/login.jsp</result>
		</action>

		<action name="logout" class="org.apache.struts.action.LogoutAction"
			method="execute">
			<result name="success">/login.jsp</result>
		</action>

	</package>
</struts>